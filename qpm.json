{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Quest-CustomCampaigns",
    "id": "customcampaigns",
    "version": "0.2.0",
    "url": "https://github.com/bsq-ports/quest-songcore",
    "additionalData": {
      "overrideSoName": "libcustomcampaigns.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "qpm qmod build",
        "pwsh ./scripts/createqmod.ps1 SongCore"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ]
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.6",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.0.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3500.*",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.17",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.6",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.6",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    }
  ]
}